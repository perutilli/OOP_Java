package clinic;

public class Patient extends Person{
	
	private Doctor doctor = null;

	public Patient(String SSN, String name, String surname) {
		super(SSN, name, surname);
	}
	
	public void assignDoctor(Doctor doctor) {
		this.doctor = doctor;
	}
	
	public Doctor getDoctor() {
		return this.doctor;
	}
	

}
